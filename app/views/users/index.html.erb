
<% if current_user.super_admin? %>
  <%= render "super_admin", object: @users %>
<% end %>

<% if current_user.teacher? %>
  <%= render "teacher" %>
<% end %>

<% if current_user.substitute? %>
  <%= render "sub" %>
<% end %>

<% if current_user.admin? %>
  <%= render "admin", object: @users %>
<% end %>
